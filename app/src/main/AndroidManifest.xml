<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dentalavenue.dentalavenue">


    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".bean"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".Wishlist" />
        <activity android:name=".cart" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Myprofile" />
        <activity android:name=".Edit" />
        <activity android:name=".Register" />
        <activity android:name=".Logout" />
        <activity android:name=".LoginType" />
        <activity android:name=".Cutomerlogin"
            android:windowSoftInputMode="stateHidden"/>
        <activity android:name=".Docterlogin"
            android:windowSoftInputMode="stateHidden"/>
        <activity android:name=".Registerdoctor"
            android:windowSoftInputMode="stateHidden"
            />
        <activity android:name=".Registercustomer"
            android:windowSoftInputMode="stateHidden"/>
        <activity android:name=".Homepage"/>
    </application>

</manifest>